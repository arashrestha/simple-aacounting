<!DOCTYPE html>
<html>
  <head>
    <title>Simaccount</title>
    <%= csrf_meta_tags %>
	<link href="https://fonts.googleapis.com/css?family=Dosis|Open+Sans:300,400,600" rel="stylesheet">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id= "wrap">
      <%= render 'shared/nav' %>
        <div class="container">
          <%= render 'shared/flash_messages' %>
              <%= yield %> 
        </div>
    </div>
  </body>
</html>
